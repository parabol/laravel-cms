{
    "name": "jakubsacha/adminlte",
    "description": "Syntara Admin panel (its based on Laravel 4) in nice theme. Package contains also small additional bugfixes and UI improvements of Syntara.",
    "keywords": ["laravel","AdminLTE","theme","admin","cms","panel","administration"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jakub Sacha",
            "email": "jakub.sacha@vert.pl"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "4.2.*",
	"mrjuliuss/syntara": "1.2.*",
        "thomaswelton/laravel-gravatar": "~0.0"
    },
    "autoload": {
        "classmap": [
            "src/migrations",
            "src/helpers"
        ],
        "psr-0": {
            "Jakubsacha\\Adminlte\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "stable"
}
